
<div class="form-group row">
    <label for="sibling_name"
           class="col-sm-4 control-label">Name</label>
    <div class="col-sm-8">
        {{ form_widget(item.name, {'type':'text', 'attr': {'class': 'form-control'}}) }}
        {{ form_errors(item.name) }}
    </div>
</div>



<div class="form-group row">
    <label for="sibling_name"
           class="col-sm-4 control-label">Price</label>
    <div class="col-sm-8">
        {{ form_widget(item.price, {'attr': {'class': 'form-control'}}) }}
        {{ form_errors(item.price) }}
    </div>
</div>


<div class="form-group row">
    <label for="sibling_name"
           class="col-sm-4 control-label">Images</label>
    <div class="col-sm-8">
        <ul class="item_images" data-prototype="{{ include('item/_form_item_images.html.twig', { 'item': item.itemImages.vars.prototype }) |e('html_attr') }}">

        </ul>
    </div>
</div>

